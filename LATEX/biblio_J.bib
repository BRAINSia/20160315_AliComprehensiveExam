file = {:Users/joymatsui/Documents/MendeleyDesktop/Mulkern et al. - 1999 - Multi-component apparent diffusion coefficients in human brain.pdf:pdf},
issn = {1099-1492},
journal = {NMR in Biomedicine},
keywords = {Adult,Brain,Brain: anatomy \& histology,Brain: physiology,Computer-Assisted,Diffusion,Humans,Imaging,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Phantoms,Signal Processing},
month = feb,
