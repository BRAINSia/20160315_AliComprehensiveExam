isbn = {4412237621},
issn = {0730725X},
journal = {Magnetic Resonance Imaging},
keywords = {Animals,Biological,Brain,Brain Mapping,Brain: anatomy \& histology,Brain: metabolism,Cats,Computer Simulation,Diffusion,Female,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Mathematics,Models,Sensitivity and Specificity,Water,anisotropy,diffusion tensor,noise,optimisation},
