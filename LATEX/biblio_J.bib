keywords = {80 and over,Adult,Aged,Blood-Brain Barrier,Blood-Brain Barrier: physiology,Brain,Brain: pathology,Computer-Assisted,Diffusion,Echo-Planar Imaging,Female,Humans,Image Enhancement,Image Processing,Male,Middle Aged,Myocardial Infarction,Myocardial Infarction: diagnosis,Reference Values,Sensitivity and Specificity,Stroke,Stroke: diagnosis},
