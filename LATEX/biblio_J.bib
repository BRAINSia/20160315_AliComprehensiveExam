journal = {Cortex; a journal devoted to the study of the nervous system and behavior},
keywords = {Adolescent,Adult,Alexia, Pure,Alexia, Pure: etiology,Brain Mapping,Cerebrum,Cerebrum: surgery,Child,Diffusion Magnetic Resonance Imaging,Epilepsy,Epilepsy: surgery,Humans,Longitudinal Studies,Male,Middle Aged,Neural Pathways,Neural Pathways: surgery,Neurosurgical Procedures,Neurosurgical Procedures: adverse effects},
month = sep,
