issn = {17555930},
journal = {CNS Neuroscience \& Therapeutics},
keywords = {Adolescent,Adult,Age of Onset,Aged,Atrophy,Brain,Brain: pathology,Disease Progression,Female,Humans,Huntington Disease,Huntington Disease: genetics,Huntington Disease: pathology,Male,Middle Aged,Mutation,Trinucleotide Repeats},
