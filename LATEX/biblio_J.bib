issn = {1878-5883},
journal = {Journal of the neurological sciences},
keywords = {Adolescent,Cognition,Cognition Disorders,Cognition Disorders: diagnosis,Cognition Disorders: metabolism,Cognition Disorders: psychology,Cognition: physiology,Diffusion Tensor Imaging,Diffusion Tensor Imaging: methods,Female,Humans,Male,Multiple Sclerosis,Multiple Sclerosis: metabolism,Multiple Sclerosis: psychology,Photic Stimulation,Photic Stimulation: methods,Psychomotor Performance,Psychomotor Performance: physiology,Time Factors},
