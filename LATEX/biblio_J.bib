issn = {1529-2401},
journal = {Journal of Neuroscience},
keywords = {Brain Mapping,Brain Mapping: methods,Child Development,Child Development: physiology,Diffusion Magnetic Resonance Imaging,Diffusion Magnetic Resonance Imaging: methods,Humans,Infant,Neural Conduction,Neural Conduction: physiology,Neuronal Plasticity,Neuronal Plasticity: physiology,Newborn,Photic Stimulation,Photic Stimulation: methods,Visual Pathways,Visual Pathways: growth \& development},
