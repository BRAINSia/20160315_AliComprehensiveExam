keywords = {Adult,Aged,Aging,Aging: physiology,Brain,Brain: anatomy \& histology,Brain: physiology,Female,Humans,Male,Mental Recall,Mental Recall: physiology,Middle Aged,Myelinated,Myelinated: ultrastructure,Nerve Fibers,Reaction Time,Reaction Time: physiology,Speech Perception,Speech Perception: physiology,Task Performance and Analysis},
month = jul,
number = {7},
pages = {1070--1079},
pmid = {17383774},
title = {{Age-related slowing of memory retrieval: contributions of perceptual speed and cerebral white matter integrity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2396513\&tool=pmcentrez\&rendertype=abstract},
