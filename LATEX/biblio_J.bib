pmid = {16755582},
title = {{Diffusion tensor imaging in presymptomatic and early Huntington's disease: Selective white matter pathology and its relationship to clinical measures.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16755582},
volume = {21},
