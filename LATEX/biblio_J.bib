@article{Kuperberg2003,
abstract = {Schizophrenia is characterized by small reductions in cortical gray matter volume, particularly in the temporal and prefrontal cortices. The question of whether cortical thickness is reduced in schizophrenia has not been addressed using magnetic resonance imaging (MRI) techniques. Our objectives were to test the hypothesis that cortical thinning in patients with schizophrenia (relative to control subjects) is greater in temporal and prefrontal regions of interest (ROIs) than in control ROIs (superior parietal, calcarine, postcentral, central, and precentral cortices), and to obtain an unbiased estimate of the distribution of cortical thinning in patients (relative to controls) by constructing mean and statistical cortical thickness difference maps.},
