issn = {0364-5134},
journal = {Annals of Neurology},
keywords = {Adult,Aged,Caudate Nucleus,Caudate Nucleus: pathology,Female,Humans,Huntington Disease,Huntington Disease: pathology,Immunohistochemistry,Infant,Male,Middle Aged},
