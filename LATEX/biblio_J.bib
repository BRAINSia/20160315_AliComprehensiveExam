month = feb,
number = {2},
pages = {292--300},
pmid = {9001155},
title = {{The b matrix in diffusion tensor echo-planar imaging.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9001155},
volume = {37},
year = {1997}
}

